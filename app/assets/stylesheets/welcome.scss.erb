::-webkit-scrollbar {
  width: 0%;
  display: none;
}
html, a:hover{
  cursor: image-url("cursor1.png"), auto;
}
body {
  background: image-url("sky.jpg"), image-url("2.jpg"), image_url("bottom.jpg");
  background-position:0px 2500px, top left, 0px 4000px;
  background-size: auto;
  background-repeat: no-repeat, no-repeat;
}
.container#nopadding{
  padding-bottom: 0%
}
.container{
  width: 40%;
  background:transparent;
  padding:20px;
  margin:0 auto;
  padding-top: 2.5%;
  color: whitesmoke;
  font-size:17pt;
}

.predator{
  position: absolute;
  width: 481px;
